# Environment Configuration for Browser Use Integration
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for browser-use)
OPENAI_API_KEY=your_openai_api_key_here

# Groq API Key (for CrewAI agents)
GROQ_API_KEY=your_groq_api_key_here

# Optional: Browser Use Cloud API Key (if using cloud service)
BROWSER_USE_API_KEY=your_browser_use_cloud_api_key_here

# Browser settings (optional)
BROWSER_HEADLESS=true
BROWSER_VIEWPORT_WIDTH=1280
BROWSER_VIEWPORT_HEIGHT=720